Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"a", "=", " ", "1"}], ";", 
  RowBox[{"b", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tol", " ", "=", " ", "0.01"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
   RowBox[{
    SuperscriptBox["x", "3"], "-", "x", "-", "1"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"RegulaFalsi", "[", 
    RowBox[{"f_", ",", "n_", ",", "\[Epsilon]_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"nx", ",", " ", "exact", ",", "rf", ",", "e", ",", " ", 
       RowBox[{"ax", "=", "a"}], ",", " ", 
       RowBox[{"bx", "=", "b"}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"nx", "=", " ", "1"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"rf", "=", 
          RowBox[{
           RowBox[{"NSolve", "[", 
            RowBox[{
             RowBox[{"0", "\[Equal]", " ", 
              RowBox[{
               RowBox[{
                FractionBox[
                 RowBox[{
                  RowBox[{"f", "[", "ax", "]"}], "-", 
                  RowBox[{"f", "[", "bx", "]"}]}], 
                 RowBox[{"ax", "-", "bx"}]], "*", 
                RowBox[{"(", 
                 RowBox[{"x", "-", "ax"}], ")"}]}], "+", 
               RowBox[{"f", "[", "ax", "]"}]}]}], ",", "x"}], "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "1", ",", "2"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{"f", "[", "rf", "]"}], "]"}], "<", "\[Epsilon]"}], ",", 
           " ", 
           RowBox[{
            RowBox[{"Break", "[", "]"}], ";"}]}], "]"}], ";", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"f", "[", "rf", "]"}], "*", 
             RowBox[{"f", "[", "bx", "]"}]}], "<", "0"}], ",", " ", 
           RowBox[{"ax", "=", "rf"}], ",", " ", 
           RowBox[{"bx", "=", "rf"}]}], "]"}], ";", 
         RowBox[{"nx", "=", " ", 
          RowBox[{"nx", "+", "1"}]}], ";"}], ",", "n"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"exact", " ", "=", " ", 
       RowBox[{
        RowBox[{"FindRoot", "[", 
         RowBox[{
          RowBox[{"f", "[", "x", "]"}], ",", " ", 
          RowBox[{"{", 
           RowBox[{"x", ",", " ", "ax", ",", " ", "bx"}], "}"}], ",", " ", 
          RowBox[{"AccuracyGoal", "\[Rule]", "5"}]}], "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      
      RowBox[{"e", " ", "=", " ", 
       RowBox[{"Abs", "[", 
        RowBox[{"rf", "-", "exact"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"{", 
        RowBox[{"rf", ",", " ", "exact", ",", "e", ",", "nx"}], "}"}], "]"}], 
      ";"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"RegulaFalsi", "[", 
  RowBox[{"f", ",", "1000", ",", " ", "tol"}], "]"}]}], "Input",
 CellChangeTimes->{{3.8621017946461067`*^9, 3.862101795810667*^9}, {
  3.862101850843379*^9, 3.8621019559324083`*^9}, {3.8621019982970743`*^9, 
