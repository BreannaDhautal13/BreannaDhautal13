   RowBox[{"fun_", ",", "x0_", ",", "y0_", ",", "h_", ",", "xf_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"t", "=", "x0"}], ";", " ", 
    RowBox[{"w", "=", "y0"}], ";", " ", 
    RowBox[{"li", " ", "=", " ", 
     RowBox[{"{", "w", "}"}]}], ";", " ", 
    RowBox[{"totalS", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"xf", "-", "x0"}], ")"}], "/", "h"}]}], ";", " ", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "0"}], ",", 
      RowBox[{"i", "<", "totalS"}], ",", 
      RowBox[{"i", "++"}], ",", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"i", ">", "0"}], ",", 
         RowBox[{
          RowBox[{"w", "=", 
           RowBox[{"N", "[", 
