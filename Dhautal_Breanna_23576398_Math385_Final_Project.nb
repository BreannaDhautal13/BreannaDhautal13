 RowBox[{
  RowBox[{"BD2", "[", 
   RowBox[{"fun_", ",", "x0_", ",", "y0_", ",", "h_", ",", "xf_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"t", "=", "x0"}], ";", " ", 
    RowBox[{"w", "=", "y0"}], ";", " ", 
    RowBox[{"li", " ", "=", " ", 
     RowBox[{"{", "w", "}"}]}], ";", " ", 
    RowBox[{"totalS", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"xf", "-", "x0"}], ")"}], "/", "h"}]}], ";", " ", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "0"}], ",", 
      RowBox[{"i", "<", "totalS"}], ",", 
      RowBox[{"i", "++"}], ",", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"i", ">", "0"}], ",", 
         RowBox[{
          RowBox[{"w", "=", 
           RowBox[{"N", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"1", "/", "3"}], ")"}], "*", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"4", "*", "w"}], "-", 
                RowBox[{"li", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], "+", 
                RowBox[{"2", "*", "h", "*", 
                 RowBox[{"fun", "[", 
                  RowBox[{
                   RowBox[{"t", "+", "h"}], ",", 
                   RowBox[{"w", "+", 
                    RowBox[{"h", "*", 
                    RowBox[{"fun", "[", 
                    RowBox[{"t", ",", "w"}], "]"}]}]}]}], "]"}]}]}], ")"}]}], 
             ",", "32"}], "]"}]}], ";", 
          RowBox[{"li", " ", "=", " ", 
           RowBox[{"Join", "[", 
            RowBox[{
             RowBox[{"{", "w", "}"}], ",", "li"}], "]"}]}]}], ",", 
         RowBox[{
          RowBox[{"w", "=", 
           RowBox[{"N", "[", 
            RowBox[{
             RowBox[{"w", "+", 
              RowBox[{"h", "*", 
               RowBox[{"fun", "[", 
                RowBox[{"t", ",", "w"}], "]"}]}]}], ",", "32"}], "]"}]}], ";", 
          RowBox[{"li", " ", "=", " ", 
           RowBox[{"Join", "[", 
            RowBox[{
             RowBox[{"{", "w", "}"}], ",", "li"}], "]"}]}]}]}], "]"}], ";", 
       RowBox[{"t", "=", 
        RowBox[{"t", "+", "h"}]}]}]}], "]"}], ";", 
    RowBox[{"Return", "[", 
