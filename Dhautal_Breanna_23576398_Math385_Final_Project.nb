Cell[CellGroupData[{

Cell[TextData[{
 "Newton\[CloseCurlyQuote]s method requires you to find the derivative after \
each step which is very complex to compute, therefore let\[CloseCurlyQuote]s \
give my kernel a break by using the Secant\[CloseCurlyQuote]s method! No more \
derivatives and here\[CloseCurlyQuote]s how: \n\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", 
     RowBox[{"n", " "}]], " ", "=", " ", 
    RowBox[{
     SubscriptBox["p", 
      RowBox[{"n", "-", "1"}]], " ", "-", " ", 
     FractionBox[
      RowBox[{
       RowBox[{"f", "(", 
        SubscriptBox["p", 
         RowBox[{"n", "-", "1"}]], ")"}], 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["p", 
          RowBox[{"n", "-", "1"}]], "-", 
         SubscriptBox["p", 
          RowBox[{"n", "-", "2"}]]}], ")"}]}], 
      RowBox[{
       RowBox[{"f", " ", 
        RowBox[{"(", 
         SubscriptBox["p", 
          RowBox[{"n", "-", "1"}]], ")"}]}], "-", 
       RowBox[{"f", " ", 
        RowBox[{"(", 
         SubscriptBox["p", 
          RowBox[{"n", "-", "2"}]], ")"}]}]}]]}]}], TraditionalForm]],
  ExpressionUUID->"6e323a08-988f-4432-b6fa-a58fc910353d"],
 " , when n \[GreaterEqual] 1"
}], "Subsection",
 CellChangeTimes->{{3.8620928776154003`*^9, 3.862092906188711*^9}, {
   3.862092975846789*^9, 3.862093124845162*^9}, 3.862093735700893*^9, {
   3.8621000521054773`*^9, 3.862100075091996*^9}, {3.862100412718465*^9, 
   3.862100426483584*^9}, {3.862103250019883*^9, 3.8621033076967382`*^9}, {
   3.862103356101404*^9, 3.8621033959237537`*^9}, {3.8621036808161497`*^9, 
   3.86210391947447*^9}, {3.862106304322209*^9, 3.862106360858753*^9}, {
   3.8621063917413588`*^9, 3.862106443697262*^9}},
 Background->RGBColor[
  1, 0.925, 0.925],ExpressionUUID->"39b8298c-0265-4aaa-b4de-ee68f5537681"],

Cell[CellGroupData[{

Cell[TextData[{
 "We choose the function f(x) = x",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["", "3"], TraditionalForm]],ExpressionUUID->
  "d2447646-e7cc-46ba-96f5-b70f1e5f5267"],
 "- x -1 with the initial guesses  1 and 3. Error tolerance = 0.01\nNow we \
will create a function computing the predicted root:"
}], "Subsubsection",
 CellChangeTimes->{{3.862100628105439*^9, 3.8621006501338*^9}, {
  3.8621039533354588`*^9, 3.862103964305696*^9}, {3.862106485903171*^9, 
  3.8621065287544203`*^9}, {3.8621066588034286`*^9, 3.862106669345933*^9}},
 Background->RGBColor[
  1, 1, 0.85],ExpressionUUID->"4f2da2b1-efef-4371-9b6e-55781422db73"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
   RowBox[{
    SuperscriptBox["x", "3"], "-", "x", "-", "1"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"exact", " ", "=", " ", 
   RowBox[{"FindRoot", "[", 
    RowBox[{
     RowBox[{"f", "[", "x", "]"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"x", ",", " ", "1", ",", " ", "3"}], "}"}], ",", " ", 
     RowBox[{"AccuracyGoal", "\[Rule]", "5"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Secant", "[", 
    RowBox[{"x1_", ",", "x2_"}], "]"}], " ", "=", " ", 
   RowBox[{"N", "[", 
    RowBox[{
     RowBox[{"x2", "-", 
      RowBox[{"(", 
       FractionBox[
        RowBox[{"f", "[", "x2", "]"}], 
        FractionBox[
         RowBox[{
          RowBox[{"f", "[", "x2", "]"}], "-", 
          RowBox[{"f", "[", "x1", "]"}]}], 
         RowBox[{"x2", "-", "x1"}]]], ")"}]}], ",", "5"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n0", " ", "=", "1"}], " ", ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x1", " ", "=", " ", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x2", " ", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x3", " ", "=", " ", 
   RowBox[{"Secant", "[", 
    RowBox[{"x1", ",", "x2"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tol", " ", "=", " ", "0.01"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{"While", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Abs", "[", 
        RowBox[{"x3", "-", "x2"}], "]"}], " ", ">", " ", "tol"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"n0", "++"}], ";", "\[IndentingNewLine]", 
       RowBox[{"x1", "=", "x2"}], ";", "\[IndentingNewLine]", 
       RowBox[{"x2", " ", "=", " ", "x3"}], ";", "\[IndentingNewLine]", 
       RowBox[{"x3", " ", "=", " ", 
        RowBox[{"Secant", "[", 
         RowBox[{"x1", ",", "x2"}], "]"}]}]}]}], "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"i", ",", "1", ",", "30"}], "}"}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", "n0", "\[IndentingNewLine]", 
 RowBox[{"N", "[", "x3", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"err", " ", "=", " ", 
   RowBox[{"Abs", "[", 
    RowBox[{
     RowBox[{"N", "[", "x3", "]"}], "-", "1.32472"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", "err"}], "Input",
 CellChangeTimes->{{3.86210655693834*^9, 3.862106601973668*^9}, {
