Cell[BoxData[{
 RowBox[{
  RowBox[{"a", "=", " ", "1"}], ";", 
  RowBox[{"b", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tol", " ", "=", " ", "0.01"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
   RowBox[{
    SuperscriptBox["x", "3"], "-", "x", "-", "1"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Bisection", "[", 
    RowBox[{"f_", ",", " ", "n_", ",", " ", "\[Epsilon]_"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"ax", " ", "=", "a"}], ",", " ", 
       RowBox[{"bx", "=", "b"}], ",", " ", "cx", ",", "nx"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"nx", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"cx", "=", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"ax", "+", "bx"}], ")"}], "/", "2"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"f", "[", "ax", "]"}], "*", 
             RowBox[{"f", "[", "cx", "]"}]}], "<", "0"}], ",", " ", 
           RowBox[{"bx", "=", "cx"}], ",", 
           RowBox[{"ax", "=", "cx"}]}], "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"nx", " ", "=", " ", 
          RowBox[{"nx", "+", "1"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", " ", 
          RowBox[{
           RowBox[{
            RowBox[{"nx", "\[Equal]", " ", "n"}], "||", "  ", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"f", "[", "cx", "]"}], "]"}], "<", " ", 
             "\[Epsilon]"}]}], ",", " ", 
           RowBox[{"Break", "[", "]"}]}], "]"}]}], "\[IndentingNewLine]", ",",
         "n"}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"cx", "//", "N"}], ",", "nx"}], "}"}], "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"root", "[", 
    RowBox[{"f_", ",", " ", "\[Epsilon]_"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "broot", ",", " ", "n0", ",", " ", "exact", ",", " ", "e", ",", " ", 
       "Nth"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Nth", " ", "=", 
       RowBox[{"2", "*", 
        RowBox[{"Ceiling", "[", 
         RowBox[{"Log", "[", 
          RowBox[{"2", ",", " ", 
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{"b", "-", "a"}], "]"}], "/", "\[Epsilon]"}]}], "]"}], 
         "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"broot", ",", " ", "n0"}], "}"}], "=", " ", 
       RowBox[{"Bisection", "[", 
        RowBox[{"f", ",", " ", "Nth", ",", " ", "\[Epsilon]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"exact", " ", "=", " ", 
       RowBox[{
        RowBox[{"FindRoot", "[", 
         RowBox[{
          RowBox[{"f", "[", "x", "]"}], ",", " ", 
          RowBox[{"{", 
           RowBox[{"x", ",", " ", "a", ",", " ", "b"}], "}"}], ",", " ", 
          RowBox[{"AccuracyGoal", "\[Rule]", "5"}]}], "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      
      RowBox[{"e", " ", "=", " ", 
       RowBox[{"Abs", "[", 
        RowBox[{"broot", "-", "exact"}], "]"}]}], ";", "\[IndentingNewLine]", 
      
      RowBox[{"Return", "[", 
       RowBox[{
        RowBox[{"{", " ", 
         RowBox[{
         "broot", ",", " ", "exact", ",", " ", "e", ",", " ", "Nth", ",", " ",
           "n0"}], "}"}], " ", "//", "N"}], "]"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"root", "[", 
  RowBox[{"f", ",", "tol"}], "]"}]}], "Input",
 CellChangeTimes->{{3.862093766089568*^9, 3.862093854350627*^9}, {
