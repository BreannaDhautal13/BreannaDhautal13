Cell[TextData[{
 "The Backward Euler one-step method is defined by:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     RowBox[{"i", "+", "1"}]], " ", "=", " ", 
    RowBox[{
     SubscriptBox["y", "i"], " ", "+", " ", 
     RowBox[{"h", " ", 
      RowBox[{"f", "(", " ", 
       RowBox[{
        SubscriptBox["t", 
         RowBox[{"i", "+", "1", " "}]], ",", " ", 
        SubscriptBox["y", 
         RowBox[{"i", "+", "1"}]]}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"1663f0b0-838a-4b21-846d-81aee503de26"],
 "  for i = 0,1,2,...,N"
}], "Subsection",
