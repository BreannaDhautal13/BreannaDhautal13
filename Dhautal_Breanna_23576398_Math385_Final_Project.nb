Cell["Second Order Adams Moulton", "Subsubsection",
 CellChangeTimes->{{3.862178304915615*^9, 3.8621783255172863`*^9}},
 Background->RGBColor[
  1, 1, 0.85],ExpressionUUID->"4f875b62-63c3-4246-a1cd-3b04a851a4e0"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", 
    RowBox[{"t_", ",", "y_"}], "]"}], ":=", 
   RowBox[{"3", "+", "t", "\[Minus]", "y"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fp", "[", "t_", "]"}], " ", ":=", " ", 
   RowBox[{"N", "[", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"2", "*", 
        SuperscriptBox["\[ExponentialE]", "t"]}], "+", 
       RowBox[{"t", "*", 
        SuperscriptBox["\[ExponentialE]", "t"]}], "-", "1"}], 
      SuperscriptBox["\[ExponentialE]", "t"]], ",", "5"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"AM2", "[", 
   RowBox[{"fun_", ",", "x0_", ",", "y0_", ",", "h_", ",", "xf_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"t", "=", "x0"}], ";", " ", 
    RowBox[{"w", "=", "y0"}], ";", " ", 
    RowBox[{"totalS", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"xf", "-", "x0"}], ")"}], "/", "h"}]}], ";", " ", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "0"}], ",", 
      RowBox[{"i", "<", "totalS"}], ",", 
      RowBox[{"i", "++"}], ",", 
      RowBox[{
       RowBox[{"w", "=", 
        RowBox[{"N", "[", 
         RowBox[{
          RowBox[{"w", "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"h", "/", "2"}], ")"}], "*", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"fun", "[", 
               RowBox[{
                RowBox[{"t", "+", "h"}], ",", 
                RowBox[{"w", "+", 
                 RowBox[{"h", "*", 
                  RowBox[{"fun", "[", 
                   RowBox[{"t", ",", "w"}], "]"}]}]}]}], "]"}], "+", 
              RowBox[{"fun", "[", 
               RowBox[{"t", ",", "w"}], "]"}]}], ")"}]}]}], ",", "32"}], 
         "]"}]}], ";", 
       RowBox[{"t", "=", 
        RowBox[{"t", "+", "h"}]}]}]}], "]"}], ";", 
    RowBox[{"Return", "[", 
     RowBox[{"N", "[", 
      RowBox[{"w", ",", "6"}], "]"}], "]"}]}], ")"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"M201", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"t", ",", 
       RowBox[{"AM2", "[", 
        RowBox[{"f", ",", "0", ",", "1", ",", "0.01", ",", "t"}], "]"}]}], 
      "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "4", ",", "0.01"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"M205", " ", "=", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"t", ",", 
        RowBox[{"AM2", "[", 
         RowBox[{"f", ",", "0", ",", "1", ",", "0.05", ",", "t"}], "]"}]}], 
       "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", "0", ",", "4", ",", "0.05"}], "}"}]}], "]"}]}], 
   ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"exact", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"t", ",", 
       RowBox[{"fp", "[", "t", "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "4", ",", "0.01"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListLinePlot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"M201", ",", "M205", " ", ",", "exact"}], "}"}], ",", 
   RowBox[{"PlotRange", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"0", ",", "2"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"1", ",", "4"}], "}"}]}], "}"}]}], ",", 
   RowBox[{"PlotLegends", "->", 
    RowBox[{"{", 
     RowBox[{
     "\"\<h = 0.01\>\"", ",", "\"\<h = 0.05\>\"", ",", " ", 
      "\"\<Exact Solution\>\""}], " ", "}"}]}]}], "]"}]}], "Input",
