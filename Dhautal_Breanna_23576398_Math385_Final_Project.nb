             RowBox[{"w", "+", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"h", "/", "2"}], ")"}], "*", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"3", "*", 
                  RowBox[{"fun", "[", 
                   RowBox[{"t", ",", "w"}], "]"}]}], "-", 
