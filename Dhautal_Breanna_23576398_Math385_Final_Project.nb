 RowBox[{
  RowBox[{"AB4", "[", 
   RowBox[{"fun_", ",", "x0_", ",", "y0_", ",", "h_", ",", "xf_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"t", "=", "x0"}], ";", " ", 
    RowBox[{"w", "=", "y0"}], ";", " ", 
    RowBox[{"li", " ", "=", " ", 
     RowBox[{"{", "w", "}"}]}], ";", " ", 
    RowBox[{"totalS", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"xf", "-", "x0"}], ")"}], "/", "h"}]}], ";", " ", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "0"}], ",", 
      RowBox[{"i", "<", "totalS"}], ",", 
      RowBox[{"i", "++"}], ",", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"i", ">", "2"}], ",", 
         RowBox[{
          RowBox[{"w", "=", 
           RowBox[{"N", "[", 
            RowBox[{
             RowBox[{"w", "+", 
              RowBox[{
               RowBox[{"h", "/", "24"}], "*", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"55", "*", 
                  RowBox[{"fun", "[", 
                   RowBox[{"t", ",", "w"}], "]"}]}], "-", 
                 RowBox[{"59", "*", 
                  RowBox[{"fun", "[", 
                   RowBox[{
                    RowBox[{"t", "-", "h"}], ",", 
                    RowBox[{"li", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], "+", 
                 RowBox[{"37", "*", 
                  RowBox[{"fun", "[", 
                   RowBox[{
                    RowBox[{"t", "-", 
                    RowBox[{"2", "*", "h"}]}], ",", 
                    RowBox[{"li", "[", 
                    RowBox[{"[", "3", "]"}], "]"}]}], "]"}]}], "-", 
                 RowBox[{"9", "*", 
                  RowBox[{"fun", "[", 
                   RowBox[{
                    RowBox[{"t", "-", 
                    RowBox[{"3", "*", "h"}]}], ",", 
                    RowBox[{"li", "[", 
                    RowBox[{"[", "4", "]"}], "]"}]}], "]"}]}]}], ")"}]}]}], 
             ",", "32"}], "]"}]}], ";", 
          RowBox[{"li", " ", "=", " ", 
           RowBox[{"Join", "[", 
            RowBox[{
             RowBox[{"{", "w", "}"}], ",", "li"}], "]"}]}]}], ",", 
         RowBox[{
          RowBox[{"w", "=", 
           RowBox[{"N", "[", 
            RowBox[{
             RowBox[{"w", "+", 
              RowBox[{"h", "*", 
