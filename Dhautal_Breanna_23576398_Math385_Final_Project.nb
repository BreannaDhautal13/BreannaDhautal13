  FormBox[
   RowBox[{
    FractionBox[
     SuperscriptBox["k", "n"], 
     RowBox[{"1", "-", "k"}]], 
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{
      SubscriptBox["p", "1"], " ", "-", " ", 
      SubscriptBox["p", "0"]}], "\[RightBracketingBar]"}]}], 
   TraditionalForm]],ExpressionUUID->"95b0cb49-41c6-4b42-b007-2faca1eb2e2b"],
