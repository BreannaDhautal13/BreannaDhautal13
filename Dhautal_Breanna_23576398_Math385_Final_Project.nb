Cell[BoxData[{
 RowBox[{
  RowBox[{"EulerM", "[", 
   RowBox[{"fun_", ",", "x0_", ",", "y0_", ",", "stepS_", ",", "xf_"}], "]"}],
   ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"t", "=", "x0"}], ";", " ", 
    RowBox[{"w", "=", "y0"}], ";", " ", 
    RowBox[{"totalS", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"xf", "-", "x0"}], ")"}], "/", "stepS"}]}], ";", " ", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "0"}], ",", 
      RowBox[{"i", "<", "totalS"}], ",", 
      RowBox[{"i", "++"}], ",", 
      RowBox[{
       RowBox[{"w", "=", 
        RowBox[{"N", "[", 
         RowBox[{
          RowBox[{"w", "+", 
           RowBox[{"stepS", "*", 
            RowBox[{"fun", "[", 
             RowBox[{"t", ",", "w"}], "]"}]}]}], ",", "32"}], "]"}]}], ";", 
       RowBox[{"t", "=", 
        RowBox[{"t", "+", "stepS"}]}]}]}], "]"}], ";", 
    RowBox[{"Return", "[", 
     RowBox[{"N", "[", 
      RowBox[{"w", ",", "5"}], "]"}], "]"}]}], ")"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", 
    RowBox[{"t_", ",", "y_"}], "]"}], ":=", 
