Cell[BoxData[{
 RowBox[{
  RowBox[{"Kutta", "[", 
   RowBox[{"fun_", ",", "x0_", ",", "y0_", ",", "h_", ",", "xf_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"t", "=", "x0"}], ";", " ", 
    RowBox[{"w", "=", "y0"}], ";", " ", 
    RowBox[{"totalS", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"xf", "-", "x0"}], ")"}], "/", "h"}]}], ";", "  ", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "0"}], ",", 
      RowBox[{"i", "<", "totalS"}], ",", 
      RowBox[{"i", "++"}], ",", 
      RowBox[{
       RowBox[{"w", "=", " ", 
        RowBox[{"N", "[", 
         RowBox[{
          RowBox[{"w", "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"h", "/", "6"}], ")"}], "*", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"fun", "[", 
               RowBox[{"t", ",", "w"}], "]"}], "+", 
              RowBox[{"2", "*", 
               RowBox[{"fun", "[", 
                RowBox[{
                 RowBox[{"t", "+", 
                  RowBox[{"h", "/", "2"}]}], ",", 
                 RowBox[{"w", "+", 
                  RowBox[{
                   RowBox[{"h", "/", "2"}], "*", 
                   RowBox[{"fun", "[", 
                    RowBox[{"t", ",", "w"}], "]"}]}]}]}], "]"}]}], "+", 
              RowBox[{"2", "*", 
               RowBox[{"fun", "[", 
                RowBox[{
                 RowBox[{"t", "+", 
                  RowBox[{"h", "/", "2"}]}], ",", 
                 RowBox[{"w", "+", 
                  RowBox[{
                   RowBox[{"h", "/", "2"}], "*", 
                   RowBox[{"fun", "[", 
                    RowBox[{
                    RowBox[{"t", "+", 
                    RowBox[{"h", "/", "2"}]}], ",", 
                    RowBox[{"w", "+", 
                    RowBox[{
                    RowBox[{"h", "/", "2"}], "*", 
                    RowBox[{"fun", "[", 
                    RowBox[{"t", ",", "w"}], "]"}]}]}]}], "]"}]}]}]}], 
                "]"}]}], "+", 
              RowBox[{"fun", "[", 
               RowBox[{
                RowBox[{"t", "+", "h"}], ",", 
                RowBox[{"w", "+", 
                 RowBox[{"h", "*", 
                  RowBox[{"fun", "[", 
                   RowBox[{
                    RowBox[{"t", "+", 
                    RowBox[{"h", "/", "2"}]}], ",", 
                    RowBox[{"w", "+", 
                    RowBox[{
                    RowBox[{"h", "/", "2"}], "*", 
                    RowBox[{"fun", "[", 
                    RowBox[{
                    RowBox[{"t", "+", 
                    RowBox[{"h", "/", "2"}]}], ",", 
                    RowBox[{"w", "+", 
                    RowBox[{
                    RowBox[{"h", "/", "2"}], "*", 
                    RowBox[{"fun", "[", 
                    RowBox[{"t", ",", "w"}], "]"}]}]}]}], "]"}]}]}]}], 
                   "]"}]}]}]}], "]"}]}], ")"}]}]}], ",", "32"}], "]"}]}], ";", 
       RowBox[{"t", "=", 
        RowBox[{"t", "+", "h"}]}]}]}], "]"}], ";", 
    RowBox[{"Return", "[", 
     RowBox[{"N", "[", 
      RowBox[{"w", ",", "9"}], "]"}], "]"}]}], ")"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", 
    RowBox[{"t_", ",", "y_"}], "]"}], ":=", 
   RowBox[{"3", "+", "t", "\[Minus]", "y"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fp", "[", "t_", "]"}], " ", ":=", " ", 
   RowBox[{"N", "[", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"2", "*", 
        SuperscriptBox["\[ExponentialE]", "t"]}], "+", 
       RowBox[{"t", "*", 
        SuperscriptBox["\[ExponentialE]", "t"]}], "-", "1"}], 
      SuperscriptBox["\[ExponentialE]", "t"]], ",", "9"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Kutta01", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"t", ",", 
       RowBox[{"Kutta", "[", 
        RowBox[{"f", ",", "0", ",", "1", ",", "0.01", ",", "t"}], "]"}]}], 
      "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "4", ",", "0.01"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Kutta05", " ", "=", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"t", ",", 
        RowBox[{"Kutta", "[", 
         RowBox[{"f", ",", "0", ",", "1", ",", "0.05", ",", "t"}], "]"}]}], 
       "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", "0", ",", "4", ",", "0.05"}], "}"}]}], "]"}]}], 
   ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"exact", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"t", ",", 
       RowBox[{"fp", "[", "t", "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "4", ",", "0.025"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListLinePlot", "[", 
  RowBox[{
   RowBox[{"{", 
